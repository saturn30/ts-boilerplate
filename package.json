{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ts-node src/index",
    "dev": "tsup --dts --watch",
    "build": "tsup --dts",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": true,
    "sourcemap": true,
    "clean": true
  },
  "eslintIgnore": [
    "/**/*.js"
  ],
  "dependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "import-sort": "6.0.0",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module-scoped": "1.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "tsup": "^6.7.0"
  }
}
