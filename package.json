{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ts-node src/index",
    "dev": "tsup --dts --watch",
    "build": "tsup --dts",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": true,
    "sourcemap": true,
    "clean": true
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "jest": "29.5.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "tsup": "^6.7.0"
  }
}
